
PROJECT_NAME="Nena"
API_V1_STR="/api/v1"

# Database
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=nena
DATABASE_URL="postgresql://postgres:your_postgres_password@localhost/nena"

# Security
SECRET_KEY=a_very_secret_key_that_you_should_change
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# SMTP (optional, for email sending)
SMTP_TLS=True
SMTP_PORT=587
SMTP_HOST=your_smtp_host
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Kafka (optional, for messaging)
KAFKA_BOOTSTRAP_SERVERS=localhost:9092

# AWS (optional, for S3 storage)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=your_aws_region
S3_BUCKET_NAME=your_s3_bucket_name

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:8080"]
